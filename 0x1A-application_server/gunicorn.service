[Unit]
Description = Gunicorn
After = network.target

[Service]
User = ubuntu
Group = www-data
WorkingDirectory = /path/to/AirBnB_clone_v4/web_dynamic
ExecStart = gunicorn --workers=3 --bind 0.0.0.0:5003 2-hbnb:app --error-logfile /tmp/airbnb-error.log --access-logfile /tmp/airbnb-access.log
Restart = always

[Install]
WantedBy = multi-user.target
